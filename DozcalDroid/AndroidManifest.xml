<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dozcaldroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/> 

    <application
        android:allowBackup="true"
        android:icon="@drawable/dozcal_icon"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.dozcaldroid.MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dozcaldroid.Activity_settings_setter"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_activity_settings_setter" >
        </activity>
        <activity
            android:name="com.example.dozcaldroid.Addnew_activity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_addnew_activity" >
        </activity>
    </application>

</manifest>